<application>
  <component name="LanguageInjectionConfiguration">
    <injection language="VueJS" injector-id="xml">
      <display-name>div/@click</display-name>
      <place><![CDATA[xmlAttribute().withLocalName(string().equalTo("click")).withParent(xmlTag().withLocalName(string().equalTo("div")).withNamespace(string().equalTo("http://www.w3.org/1999/html")))]]></place>
    </injection>
    <injection language="VueJS" injector-id="xml">
      <display-name>v-list</display-name>
      <place><![CDATA[xmlTag().withLocalName(string().equalTo("v-list")).withNamespace(string().equalTo("http://www.w3.org/1999/html"))]]></place>
    </injection>
  </component>
</application>